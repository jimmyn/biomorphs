{
  "name": "biomorphs",
  "version": "1.1.1",
  "description": "Richard Dawkins' evolution game",
  "main": "index.js",
  "repository": "git@github.com:jimmyn/biomorphs.git",
  "scripts": {
    "start": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --env.dev",
    "start:prod": "babel-node lib/server",
    "lint": "eslint src test;exit 0;",
    "lint:fix": "eslint --fix src;exit 0;",
    "build": "babel-node ./node_modules/webpack/bin/webpack.js --env.prod",
    "deploy": "npm run build -- --env.deploy && gh-pages -d public",
    "release:patch": "npm version patch && npm run deploy",
    "release:minor": "npm version minor && npm run deploy",
    "release:major": "npm version major && npm run deploy"
  },
  "author": "Dmitriy Nevzorov",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "eslint": "^3.2.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "gh-pages": "^0.11.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^4.2.0",
    "rainbowvis.js": "^1.0.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.0.4",
    "redux-thunk": "^2.1.0",
    "scroll-behavior": "^0.7.0"
  }
}
